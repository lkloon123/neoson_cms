{
  "name": "neoson/neoson-cms",
  "type": "project",
  "version": "1.0.0",
  "description": "A Laravel CMS",
  "keywords": [
    "framework",
    "laravel",
    "neoson",
    "cms"
  ],
  "license": "MIT",
  "require": {
    "php": "^7.2",
    "alexusmai/laravel-file-manager": "^2.4",
    "anlutro/l4-settings": "^0.10",
    "askedio/laravel-soft-cascade": "^6.0",
    "bensampo/laravel-enum": "^1.26",
    "fideloper/proxy": "^4.0",
    "laravel/framework": "^6.0",
    "laravel/telescope": "^3.1",
    "laravel/tinker": "^2.0",
    "mews/purifier": "^3.1",
    "nelexa/zip": "^3.1",
    "owen-it/laravel-auditing": "^9.3",
    "predis/predis": "^1.1",
    "santigarcor/laratrust": "5.2.*",
    "spatie/laravel-http-logger": "^1.4",
    "spatie/laravel-tags": "^2.5",
    "talevskiigor/composer-bump": "^1.0",
    "wikimedia/composer-merge-plugin": "dev-master"
  },
  "require-dev": {
    "beyondcode/laravel-dump-server": "^1.0",
    "doctrine/dbal": "^2.9",
    "filp/whoops": "^2.0",
    "fzaninotto/faker": "^1.4",
    "mockery/mockery": "^1.0",
    "nunomaduro/collision": "^3.0",
    "barryvdh/laravel-ide-helper": "^2.6",
    "phpunit/phpunit": "^7.5"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "dont-discover": []
    },
    "merge-plugin": {
      "include": [
        "storage/plugins/*/*/composer.json"
      ],
      "recurse": true,
      "replace": false,
      "merge-dev": false,
      "merge-extra": true,
      "merge-extra-deep": true
    }
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    },
    "classmap": [
      "database/seeds",
      "database/factories"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {
    "post-autoload-dump": [
      "App\\Bootstrap\\ComposerScripts::postAutoloadDump",
      "@php artisan custom:discover --ansi"
    ],
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate --ansi"
    ],
    "post-update-cmd": [
      "App\\Bootstrap\\ComposerScripts::postUpdate",
      "php artisan ide-helper:generate",
      "php artisan ide-helper:meta"
    ]
  }
}
